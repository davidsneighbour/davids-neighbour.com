{
  "name": "davids-neighbour-com-fonts",
  "slug": "davids-neighbour-com-fonts",
  "url": "https://davids-neighbour.com",
  "version": "2020.0.3",
  "description": "Webfonts for davids-neighbour.com",
  "author": {
    "name": "Patrick Kollitsch",
    "email": "patrick@davids-neighbour.com",
    "web": "https://davids-neighbour.com"
  },
  "contributors": [
    {
      "name": "Patrick Kollitsch",
      "email": "patrick@davids-neighbour.com",
      "web": "https://davids-neighbour.com"
    }
  ],
  "homepage": "https://davids-neighbour.com",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/davidsneighbour/davids-neighbour.com-fonts.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "scripts": {
    "release": "standard-version --release-as patch -a && git push --follow-tags origin master",
    "release-next": "standard-version --release-as minor -a && git push --follow-tags origin master",
    "release-major": "standard-version --release-as major -a && git push --follow-tags origin master"
  },
  "standard-version": {
    "skip": {
      "changelog": true
    },
    "bumpFiles": [
      {
        "filename": "package.json",
        "type": "json"
      }
    ]
  },
  "devDependencies": {
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "cz-customizable-ghooks": "^2.0.0",
    "standard-version": "^9.0.0"
  }
}
