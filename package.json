{
  "name": "davids-neighbour.com",
  "version": "1.0.36",
  "description": "Website and content for davids-neighbour.com",
  "main": "index.js",
  "scripts": {
    "css": "node-sass --quiet --include-path node_modules --output-style compressed --precision 6 src/scss/theme.scss tmp/theme.css",
    "algolia": "atomic-algolia",
    "release": "npm version patch -m \"chore(release): v%s\" && git push origin"
  },
  "repository": "git+ssh://git@bitbucket.org/pkollitsch/davids-neighbour.com.git",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "license": "proprietary",
  "homepage": "https://bitbucket.org/pkollitsch/davids-neighbour.com#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "browserslist": [
    ">= 1%",
    "last 2 major version",
    "not dead",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "private": true,
  "dependencies": {
    "atomic-algolia": "^0.3.17",
    "autoprefixer": "^9.6.1",
    "cssnano": "^4.1.10",
    "cz-customizable": "^6.2.0",
    "lodash": "^4.17.15",
    "postcss": "^7.0.14",
    "postcss-cli": "^6.1.2",
    "uglify-js": "^3.5.3"
  }
}
