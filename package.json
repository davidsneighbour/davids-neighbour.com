{
  "name": "davids-neighbour.com",
  "version": "2.0.3",
  "description": "Website and content for davids-neighbour.com",
  "private": true,
  "scripts": {
    "start": "webpack --mode=development --watch",
    "build": "webpack --mode=production",
    "nightwatch": "nightwatch -c tests/nightwatch.conf.js",
    "release": "standard-version --release-as patch -a -t \"\" && git push --follow-tags origin master",
    "release-next": "standard-version --release-as minor -a -t \"\" && git push --follow-tags origin master",
    "release-major": "standard-version --release-as major -a -t \"\" && git push --follow-tags origin master"
  },
  "repository": "git+ssh://git@bitbucket.org/pkollitsch/davids-neighbour.com.git",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "license": "proprietary",
  "homepage": "https://bitbucket.org/pkollitsch/davids-neighbour.com#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "browserslist": [
    ">= 1%",
    "last 2 major version",
    "not dead",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "dependencies": {
    "atomic-algolia": "0.3.17",
    "bootstrap": "5.0.0-alpha1",
    "cssnano": "4.1.10",
    "cz-customizable": "6.2.0",
    "lodash": "4.17.19",
    "popper.js": "^1.16.1",
    "postcss": "7.0.27",
    "postcss-cli": "7.1.1",
    "standard-version": "8.0.2",
    "uglify-js": "3.8.0"
  },
  "standard-version": {
    "skip": {
      "changelog": true
    },
    "bumpFiles": [
      {
        "filename": "package.json",
        "type": "json"
      }
    ]
  },
  "devDependencies": {
    "@babel/core": "7.10.5",
    "@babel/preset-env": "7.10.4",
    "autoprefixer": "9.7.4",
    "babel-loader": "8.1.0",
    "core-js": "3.6.5",
    "css-loader": "4.0.0",
    "mini-css-extract-plugin": "0.9.0",
    "postcss-loader": "3.0.0",
    "webpack": "4.44.0",
    "webpack-cli": "3.3.12"
  }
}
