{
	"extends": "astro/tsconfigs/strictest",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@assets/*": ["./src/assets/*"],
			"@components/*": ["./src/components/*"],
			"@config/*": ["./src/config/*"],
			"@data/*": ["./src/data/*"],
			"@layouts/*": ["./src/layouts/*"],
			"@packages/*": ["./src/packages/*"],
			"@pages/*": ["./src/pages/*"],
			"@scripts/*": ["./src/scripts/*"],
			"@styles/*": ["./src/styles/*"],
			"@utils/*": ["./src/utils/*"]
		}
	}
}
